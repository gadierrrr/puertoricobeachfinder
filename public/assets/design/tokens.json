    "--z-nav": "40",
    "--z-drawer": "50",
    "--z-drawer-content": "51",
    "--z-modal": "60",
    "--z-lightbox": "65",
    "--z-lightbox-controls": "66",
    "--z-toast": "100",
    "--z-skip-link": "9999",
    "--brand-yellow": "#FACC15",
    "--color-brand-yellow": "#FACC15",
    "--color-brand-darker": "#0f172a",
    "--color-slate-700": "#334155",
    "--color-slate-50": "#f8fafc"
  },
  "tailwind": {
    "fontFamily": {
      "sans": [
        "Inter",
        "system-ui",
        "-apple-system",
        "BlinkMacSystemFont",
        "sans-serif"
      ],
      "serif": [
        "Playfair Display",
        "Georgia",
        "serif"
      ]
    },
    "colors": {
      "brand": {
        "dark": "#1a2c32",
        "darker": "#132024",
        "yellow": "#fde047",
        "text": "#f1f5f9",
        "muted": "#94a3b8"
      },
      "tropical": {
        "cyan": {
          "50": "#ecfeff",
          "100": "#cffafe",
          "200": "#a5f3fc",
          "300": "#67e8f9",
          "400": "#22d3ee",
          "500": "#06b6d4",
          "600": "#0891b2",
          "700": "#0e7490"
        },
        "sun": {
          "50": "#fffbeb",
          "100": "#fef3c7",
          "200": "#fde68a",
          "300": "#fcd34d",
          "400": "#fbbf24",
          "500": "#f59e0b",
          "600": "#d97706",
          "700": "#b45309"
        },
        "coral": {
          "50": "#fff7ed",
          "100": "#ffedd5",
          "200": "#fed7aa",
          "300": "#fdba74",
          "400": "#fb923c",
          "500": "#f97316",
          "600": "#ea580c"
        },
        "sand": {
          "50": "#FFFDF7",
          "100": "#FFFBEB",
          "200": "#FEF3C7",
          "300": "#FDE68A"
        }
      },
      "warm": {
        "50": "#fafaf9",
        "100": "#f5f5f4",
        "200": "#e7e5e4",
        "300": "#d6d3d1",
        "400": "#a8a29e",
        "500": "#78716c",
        "600": "#57534e",
        "700": "#44403c",
        "800": "#292524",
        "900": "#1c1917"
      }
    },
    "borderRadius": {
      "xl": "1rem",
      "2xl": "1.5rem",
      "3xl": "2rem"
    },
    "boxShadow": {
      "card": "0 4px 12px rgba(0, 0, 0, 0.08)",
      "card-hover": "0 8px 24px rgba(0, 0, 0, 0.12)",
      "soft": "0 2px 8px rgba(0, 0, 0, 0.06)",
      "soft-lg": "0 4px 16px rgba(0, 0, 0, 0.08)",
      "sunny": "0 4px 16px rgba(251, 191, 36, 0.2)",
      "glass": "0 8px 32px rgba(0, 0, 0, 0.3)"
    },
    "backgroundImage": {
      "hero-gradient": "linear-gradient(to bottom, rgba(0,0,0,0), rgba(0,0,0,0.3), rgba(0,0,0,0.8))"
    },
    "animation": {
      "fade-in-up": "fade-in-up 0.6s ease-out forwards",
      "bounce-slow": "bounce 2s infinite"
    },
    "keyframes": {
      "fade-in-up": {
        "0%": {
          "opacity": "0",
          "transform": "translateY(20px)"
        },
        "100%": {
          "opacity": "1",
          "transform": "translateY(0)"
        }
      }
    },
    "animationDelay": {
      "100": "100ms",
      "200": "200ms",
      "300": "300ms",
      "400": "400ms",
      "500": "500ms",
      "600": "600ms"
    }
  }
}
